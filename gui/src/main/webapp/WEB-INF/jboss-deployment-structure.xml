<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.1">
   <deployment>
      <dependencies>
         <!-- [WISE-183] replace dependencies below with wise module -->
         <module name="org.jboss.ws.cxf.jbossws-cxf-client" services="true" export="true"/>
         <module name="org.jboss.ws.common"/>
         <module name="org.jboss.common-core"/>
         <module name="org.apache.cxf"/>
         <module name="org.apache.cxf.impl" optional="true"/>
         <!-- impl submodule is available since AS 7.2 -->

         <module name="com.google.guava"/>
         <module name="org.apache.commons.io"/>
         <module name="org.jboss.as.controller-client"/>
         <!-- wildfly-controller-client-*.jar -->
         <module name="org.jboss.xnio.nio"/>
         <!-- xnio-nio-*.jar, xnio-api-*.jar -->
         <module name="org.jdom"/>
         <module name="org.jboss.threads"/>
         <module name="org.jboss.remoting"/>
         <module name="javax.el.api"/>
         <!--jboss-el-api_3.0_spec-*.jar  -->
         <module name="org.jboss.dmr"/>
         <module name="org.dom4j"/>
         <module name="javax.ejb.api"/>
         <!-- jboss-ejb-api_3.2_spec-*.jar -->
         <module name="org.apache.commons.logging"/>
         <module name="nu.xom"/>
         <module name="org.jboss.sasl"/>
         <module name="org.jaxen"/>
         <module name="javax.enterprise.api"/>
         <!-- cdi-api-*.jar -->
         <module name="javax.inject.api"/>
         <module name="org.apache.httpcomponents"/>
         <module name="org.apache.commons.codec"/>
         <module name="org.apache.commons.lang"/>
         <module name="org.jboss.as.protocol"/>
         <!-- wildfly-protocol-*.jar -->
         <module name="javax.transaction.api"/>
         <!-- jta-*.jar  -->
         <module name="org.apache.commons.logging"/>

      </dependencies>
   </deployment>
</jboss-deployment-structure>
